{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceFolder}/editors/vscode"],
			"outFiles": ["${workspaceFolder}/editors/vscode/dist/**/*.js"],
			"preLaunchTask": "VS Code Extension Prelaunch"
		}
	]
}
